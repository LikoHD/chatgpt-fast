---
import Layout from "~/layouts/Layout.astro"
import Header from "~/components/Header.astro"
import Generator from "~/components/Generator"
import "~/styles/global.css"
import "@unocss/reset/tailwind.css"
import "katex/dist/katex.min.css"
import "highlight.js/styles/atom-one-dark.css"
import { parsePrompts } from "~/utils/parse"

const prompts = await parsePrompts()
---

<Layout title="ChatGPT">
  <main class="before">
    <Header />
    <Generator prompts={prompts} client:visible />
  </main>
</Layout>
      
      
import Analytics from '@vercel/analytics';

const analytics = new Analytics('your-vercel-project-id');

// 使用 analytics.track 来跟踪自定义事件
analytics.track('custom_event', {
  property1: 'value1',
  property2: 'value2',
});

